{
    "name": "kbrw/riak-bundle",
    "type": "symfony-bundle",
    "description": "Allows your application to intereact with Riak datastorage",
    "keywords": ["webservice","nosql","ODM"],
    "homepage": "https://github.com/remialvado/RiakBundle",
    "license": "Apache",
    "authors": [
        {
            "name": "Remi Alvado",
            "email": "remi.alvado@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1,<3.0-dev",
        "guzzle/guzzle": "3.*",
        "jms/serializer-bundle": "0.11.*"
    },
    "require-dev": {
        "symfony/yaml": "*",
        "symfony/browser-kit": "*",
        "symfony/class-loader": "*",
        "symfony/finder": "*",
        "symfony/css-selector": "*",
        "symfony/process": "*",
        "doctrine/doctrine-bundle": "*",
        "symfony/twig-bundle": "*",
        "doctrine/orm": "*",
        "symfony/form": "*",
        "symfony/validator": "*",
        "symfony/monolog-bundle": "*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { "Kbrw\\RiakBundle": "" }
    },
    "target-dir": "Kbrw/RiakBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
